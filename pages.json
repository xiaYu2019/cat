{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
		    "path" : "pages/login/login",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "",
		        "enablePullDownRefresh": false,
				"navigationStyle":"custom"
		    }
		    
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "车"
			}
		}, {
			"path": "pages/staff/staff",
			"style": {
				"navigationBarBackgroundColor":"#F8F8F8",
				"navigationBarTitleText": "员工",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/self/self",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false,
				"navigationStyle":"custom"
			}
		}
	   
        ,{
            "path" : "pages/vehicle/vehicle",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "车检列表",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"tabBar": {
		"selectedColor":"#1296db",
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页",
			"iconPath":"static/index.png",
			"selectedIconPath":"static/pitchindex.png"
		}, {
			"pagePath": "pages/staff/staff",
			"text": "员工",
			"iconPath":"static/staff.png",
			"selectedIconPath":"static/pitchstaff.png"
		}, {
			"pagePath": "pages/self/self",
			"text": "我的",
			"iconPath":"static/self.png",
			"selectedIconPath":"static/pitchself.png"
		}]
	},

	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
